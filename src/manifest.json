{
  "name": "Overrides Manager",
  "description": "A Sketch plugin which makes managing overrides easier.",
  "identifier": "com.gunesozgur.sketch.overrides-manager",
  "author": "Ozgur Gunes",
  "authorEmail": "o.gunes@gmail.com",
  "$schema": "https://raw.githubusercontent.com/sketch-hq/SketchAPI/develop/docs/sketch-plugin-manifest-schema.json",
  "appcast": "https://raw.githubusercontent.com/ozgurgunes/Sketch-Overrides-Manager/master/.appcast.xml",
  "homepage": "https://github.com/ozgurgunes/Sketch-Overrides-Manager",
  "icon": "icon.png",
  "commands": [
    {
      "name": "Enable All Overrides",
      "identifier": "overrides-manager.enable-all",
      "script": "./command.js",
      "handler": "enableAll"
    },
    {
      "name": "Disable All Overrides",
      "identifier": "overrides-manager.disable-all",
      "script": "./command.js",
      "handler": "disableAll"
    },
    {
      "name": "Enable Unlocked Layers",
      "identifier": "overrides-manager.enable-unlocked",
      "script": "./command.js",
      "handler": "enableUnlocked"
    },
    {
      "name": "Disable Locked Layers",
      "identifier": "overrides-manager.disable-locked",
      "script": "./command.js",
      "handler": "disableLocked"
    },
    {
      "name": "Depend On Lock Status",
      "identifier": "overrides-manager.lock-status",
      "script": "./command.js",
      "handler": "dependOnLockStatus"
    },
    {
      "name": "Enable Text Overrides",
      "identifier": "overrides-manager.enable-text-overrides",
      "script": "./command.js",
      "handler": "enableTextOverrides"
    },
    {
      "name": "Disable Text Overrides",
      "identifier": "overrides-manager.disable-text-overrides",
      "script": "./command.js",
      "handler": "disableTextOverrides"
    }
      ],
  "menu": {
    "title": "Overrides Manager",
    "items": [
      "overrides-manager.enable-all",
      "overrides-manager.disable-all",
      "-",
      "overrides-manager.enable-unlocked",
      "overrides-manager.disable-locked",
      "overrides-manager.lock-status",
      "-",
      "overrides-manager.enable-text-overrides",
      "overrides-manager.disable-text-overrides"
    ]
  }
}